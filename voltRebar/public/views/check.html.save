<div id="customerRecords" class="row">
<div class="large-4 columns">
      <label>Select Reader</label>
      <input id="readerBox1" type="checkbox"><label for="readerBox1">Reader 0</label>
      <input id="readerBox2" type="checkbox"><label for="readerBox2">Reader 1</label>
    </div>  
    <div class="large-4 columns">
       </div>
        <div class="large-2 large-offset-2 columns">
          <input type="text" id="search-label" placeholder="User Info">
        </div>
<table id="customerTable" class="large-12 columns">
  <thead>
    <tr>
      <th>Name</th>
      <th>TimeStamp</th>
      <th>Battery</th>
      <th>Reader</th>
      <th>Synced</th>
      <th>In/Out</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="customer in customers">
      <td>Ulrik Andersen</td>
      <td>{{customer.time}}</td>
      <td>{{customer.UID}}</td>
      <td>{{customer.reader}}</td>
      <td>NO</td>
      <td>O</td>
    </tr>
  </tbody>
</table>
</div>
<div class="small-4 columns ">
	<a id="cNC" href="#/" class="small alert button">Back</a>       
</div>
